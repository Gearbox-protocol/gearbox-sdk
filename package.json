{
  "name": "@gearbox-protocol/sdk",
  "version": "0.0.0-semantic-release",
  "description": "Gearbox SDK",
  "license": "MIT",
  "main": "./dist/cjs/sdk/index.js",
  "module": "./dist/esm/sdk/index.mjs",
  "types": "./dist/cjs/sdk/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/sdk/index.d.mts",
        "default": "./dist/esm/sdk/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/sdk/index.d.ts",
        "default": "./dist/cjs/sdk/index.cjs"
      }
    },
    "./dev": {
      "import": {
        "types": "./dist/esm/dev/index.d.mts",
        "default": "./dist/esm/dev/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/dev/index.d.ts",
        "default": "./dist/cjs/dev/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "example": "tsx --env-file .env scripts/example.ts | pino-pretty",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit"
  },
  "dependencies": {
    "@gearbox-protocol/sdk-gov": "^2.21.0",
    "@redstone-finance/evm-connector": "^0.6.2",
    "abitype": "^1.0.6",
    "date-fns": "^4.1.0",
    "redstone-protocol": "^1.0.5",
    "viem": ">=2.21.0 <3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@gearbox-protocol/eslint-config": "^2.0.0-next.2",
    "@gearbox-protocol/prettier-config": "^2.0.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@gearbox-protocol/eslint-config"
    ]
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
